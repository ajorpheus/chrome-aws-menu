{
  "name": "AWS Menu",
  "version": "0.0.7",
  "description": "AWS Menu",
  "manifest_version": 2,
  "background":{
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.aws.amazon.com/*",
        "https://*.aws.amazon.com/*"
      ],
      "css": [
        "menu.css"
      ],
      "js": [
        "jquery.js",
        "menu.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
  ],
  "icons": {
    "16": "app16.png",
    "48": "app48.png",
    "128": "app128.png"
  }
}
