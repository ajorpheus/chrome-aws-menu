{
  "name": "AWS Menu",
  "version": "0.0.14",
  "description": "AWS Menu",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.aws.amazon.com/*",
        "https://*.aws.amazon.com/*"
      ],
      "css": [
        "menu.css"
      ],
      "js": [
        "jquery.js",
        "waitForKeyElements.js",
        "menu.js"
      ]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "alarms",
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "app16.png",
    "48": "app48.png",
    "128": "app128.png"
  }
}
